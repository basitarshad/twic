{
  "name": "twic-mobile-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "test": "jest --clearCache && NODE_ENV=test jest",
    "test:watch": "jest --clearCache && NODE_ENV=test jest --watchAll --verbose",
    "test:coverage": "jest --clearCache && NODE_ENV=test jest --verbose --coverage",
    "lint": "eslint .",
    "compile": "rm -rf dist && tsc && tsc-alias",
    "pods": "cd ios && pod install",
    "clean-pods": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install",
    "android:clean": "cd android && ./gradlew clean",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:assembleBundleRelease": "cd android && ./gradlew bundleRelease",
    "android:assembleDebug": "cd android && ./gradlew assembleDebug",
    "android:release": "npm run compile && npm run android:clean && npm run android:assembleRelease",
    "android:bundleRelease": "watchman watch-del-all && npm run compile && npm run android:clean && npm run android:assembleBundleRelease",
    "android:debug": "npm run compile && npm run android:clean && npm run android:assembleDebug",
    "android:dev": "concurrently \"tsc -w & tsc-alias -w\" \"npx react-native run-android\"",
    "ios:dev": "concurrently \"tsc -w & tsc-alias -w\" \"npx react-native run-ios\"",
    "postinstall": "npx jetify",
    "tsc": "tsc -w & tsc-alias -w",
    "upload-sourcemaps:debug:android": "./scripts/upload-debug-android.sh",
    "upload-sourcemaps:release:android": "./scripts/upload-release-android.sh",
    "upload-sourcemaps:staging:android": "./scripts/upload-staging-android.sh",
    "upload-sourcemaps:debug:ios": "./scripts/upload-debug-ios.sh",
    "upload-sourcemaps:release:ios": "./scripts/upload-release-ios.sh",
    "upload-sourcemaps:staging:ios": "./scripts/upload-staging-ios.sh",
    "codepush:androidRelease": "npm run compile && appcenter codepush release-react -a forma-mobile/android-forma -d Production -m",
    "codepush:iosRelease": "npm run compile && appcenter codepush release-react -a forma-mobile/ios-forma -d Production -m --plist-file ios/joinforma/Info.plist -t 0.0.0",
    "codepush:clear:ios": "appcenter codepush deployment clear Staging -a forma-mobile/ios-forma",
    "codepush:clear:android": "appcenter codepush deployment clear Staging -a forma-mobile/android-forma",
    "prepare": "husky install"
  },
